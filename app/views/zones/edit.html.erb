<h1>Editing Zone</h1>

<%= render 'form', zone: @zone %>

<input id="id"  type="text">




  <div class="banner"> 

	<div class="row">
		<div class="col-md-9 col-sm-9 col-xs-9"> 

			<button  id="guardarEdicionZona" class="btn btn-default pull-right">Guardar Cambios Zona</button>
			<button id="btnmarcar" class="btn btn-default pull-right">Marcar zona</button>
		</div>
	</div>
</div>
<%= link_to 'Show', @zone %> |
<%= link_to 'Back', zones_path %>

<script type="text/javascript">
document.getElementById("id").value = <%= @zone.id  %>;
$('#btnmarcar').attr("disabled", true);
$('#guardarEdicionZona').attr("disabled", true);
  createMapZone();
</script>